{:paths ["src"]
 :aliases
 {:dev
  {:extra-paths ["test"]
   :extra-deps
   {manifold/manifold {:mvn/version "0.2.3"}}}
  :build
  {:deps {io.github.seancorfield/build-clj
          {:git/tag "v0.8.0" :git/sha "9bd8b8a"}}
   :ns-default build}
  :eastwood
  {:extra-deps
   {jonase/eastwood {:mvn/version "1.2.3"}}
   :main-opts
   ["-m" "eastwood.lint" {:source-paths ["src"]}]}
  :test
  {:extra-deps
   {lambdaisland/kaocha {:mvn/version "1.66.1034"}}
   :main-opts ["-m" "kaocha.runner"]}}}
