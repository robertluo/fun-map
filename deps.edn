{:paths ["src"]
 :aliases
 {:dev
  {:extra-paths ["test"]
   :extra-deps
   {manifold/manifold {:mvn/version "0.1.9"}}}
  :build
  {:deps {io.github.seancorfield/build-clj
          {:git/tag "v0.5.2" :git/sha "8f75b81"}}
   :ns-default build}
  :eastwood
  {:extra-deps
   {jonase/eastwood {:mvn/version "0.9.9"}}
   :main-opts
   ["-m" "eastwood.lint" {:source-paths ["src"]}]}
  :test
  {:extra-deps
   {lambdaisland/kaocha {:mvn/version "1.60.945"}}
   :main-opts ["-m" "kaocha.runner"]}}}
